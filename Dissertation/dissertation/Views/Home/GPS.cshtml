

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Styles.Render("~/Content/Main.css")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

</head>
<body>
    <header>
        <nav class="secondary_header" id="menu">
            <ul>
                <li>@Html.ActionLink("Homepage", "Homepage")</li>
                <li>@Html.ActionLink("Download Client", "DownloadClient")</li>
                <li>@Html.ActionLink("Block Website", "DnsSettings")</li>
                <li>@Html.ActionLink("Time On Screen", "TosSettings")</li>
                <li>@Html.ActionLink("GPS", "GPS")</li>
            </ul>
        </nav>
    </header>
    <div>
        <section>

            <div>
                <h3 class="GPS_title">GPS</h3>
            </div>
            <div class="table_pos">
                @foreach (var client in Model)
                {
                    <div id="location-stats">
                        <table id="client_table">
                            <tr><td>Name</td><td>@client.Name</td></tr>
                            <tr><td>IP</td><td>@client.IP</td></tr>
                            <tr><td>Country Code</td><td>@client.Country_Code</td></tr>
                            <tr><td>Country Name</td><td>@client.Country_Name</td></tr>
                            <tr><td>Region Code</td><td>@client.Region_Code</td></tr>
                            <tr><td>Region Name</td><td>@client.Region_Name</td></tr>
                            <tr><td>City</td><td>@client.City</td></tr>
                            <tr><td>Latitude</td><td>@client.Latitude</td></tr>
                            <tr><td>Longitude</td><td>@client.Longitude</td></tr>
                            <br />
                        </table>
                    </div>
                }
            </div>
        </section>
    </div>

</body>
</html>

<style>
    .GPS_title {
        color: #717070 !important;
        text-align: center;
    }

    body {
        font-family: Arial;
        font-size: 10pt;
    }

    table td:first-child {
        width: 100px;
        font-weight: bold;
    }
</style>